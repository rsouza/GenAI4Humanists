  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 7 - Multimodal Input (Images)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from google.genai import types\n",
    "\n",
    "# Upload an image\n",
    "image_path = \"../../Data/imgs/handwritten.jpg\"\n",
    "\n",
    "my_file = client.files.upload(file=image_path)\n",
    "\n",
    "prompt = \"\"\"\n",
    "Describe what you see in this image.\n",
    "\"\"\"\n",
    "\n",
    "response = client.models.generate_content(\n",
    "    model=\"gemini-2.0-flash\",\n",
    "    contents=[my_file, prompt],\n",
    ")\n",
    "\n",
    "print(response.text)"
   ]
  },
